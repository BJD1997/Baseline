{
  "$manifest": {
    "Install.bat": "${ResourceContext:MSGraph:DeviceAppManagement:MobileApps:Install.bat}",
    "Setup.msi": "https://dl.google.com/dl/chrome/install/googlechromestandaloneenterprise.msi",
    "Setup-x64.msi": "https://dl.google.com/dl/chrome/install/googlechromestandaloneenterprise64.msi",
    "Uninstall.ps1": "gci $env:ProgramFiles\\Google\\Chrome\\Application\\*\\Installer setup.exe -Recurse |% { & $_.FullName '--uninstall' '--multi-install' '--chrome' '--system-level' '--force-uninstall' }"
  },
  "@odata.type": "#microsoft.graph.win32LobApp",
  "applicableArchitectures": "x86, x64",
  "assignments": [],
  "description": "Google Chrome",
  "detectionRules": [
    {
      "@odata.type": "#microsoft.graph.win32LobAppFileSystemDetection",
      "check32BitOn64System": true,
      "detectionType": "exists",
      "fileOrFolderName": "chrome.exe",
      "operator": "notConfigured",
      "path": "%ProgramFiles%\\Google\\Chrome\\Application\\"
    }
  ],
  "developer": "",
  "fileName": "013",
  "installCommandLine": "Install.bat",
  "installExperience": {
    "deviceRestartBehavior": "basedOnReturnCode",
    "runAsAccount": "system"
  },
  "isFeatured": false,
  "minimumSupportedOperatingSystem": {
    "v10_1607": true,
    "v10_1703": false,
    "v10_1709": false,
    "v10_1803": false,
    "v10_1809": false,
    "v10_1903": false
  },
  "notes": "",
  "owner": "",
  "publisher": "Google",
  "relationships": [],
  "requirementRules": [],
  "returnCodes": [
    {
      "returnCode": 0,
      "type": "success"
    },
    {
      "returnCode": 1618,
      "type": "retry"
    },
    {
      "returnCode": 1641,
      "type": "hardReboot"
    },
    {
      "returnCode": 1707,
      "type": "success"
    },
    {
      "returnCode": 3010,
      "type": "softReboot"
    }
  ],
  "uninstallCommandLine": "powershell -ExecutionPolicy Bypass -NoProfile -NonInteractive -File ./Uninstall.ps1"
}