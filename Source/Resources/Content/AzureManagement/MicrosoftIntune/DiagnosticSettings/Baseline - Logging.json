    "workspaceId": "${urn:resource:AzureManagement:MicrosoftResources:ResourceGroupDeployments/${ResourceContext:TenantShortName}-baseline-m365logging-M365.Deployment?$outputs.outputResources['/subscriptions/${AzureManagement:SubscriptionId}/resourceGroups/${ResourceContext:TenantShortName}-baseline-m365logging/providers/Microsoft.OperationalInsights/workspaces/${ResourceContext:TenantShortName}-baseline-m365logging']}"
